using System;
using System.Linq;

namespace Tabla_del_1;

public static class Program
{
    public static void Main()
    {
        Console.WriteLine("Ingresa el numero:");
        int numero = 0;
        numero = int.Parse(Console.ReadLine());

        for (int i = 1) ; i <= 10; i++)
        {
            int cantidad = nunero * 1;

            Console.WriteLine("1 X " + i + " = " + (1 * i)"));
        }

    }
}

